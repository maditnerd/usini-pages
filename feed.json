        {
            "id": "https://fr.usini.eu/mini-ecran-connecte/",
            "url": "https://fr.usini.eu/mini-ecran-connecte/",
            "title": "Mini écran connecté",
            "content_html": "<script>// <![CDATA[\nwindow.location=\"https://wikifab.org/wiki/Mini_%C3%A9cran_connect%C3%A9\"\n// ]]></script>",
            "image": "https://fr.usini.eu/media/posts/9/tft_iftt_hack.jpg",
            "author": {
